    "eslint-define-config": "^1.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "concurrently": "^6.3.0"
  }
